{
  "name": "GitMutual",
  "description": "WIP - nothing to see here",
  "version": "0.0.0",
  "private": "true",
  "author": "Bret Comnes <bcomnes@gmail.com> (https://bret.io)",
  "bugs": {
    "url": "https://github.com/bcomnes/GitMutual/issues"
  },
  "dependencies": {
    "@octokit/plugin-retry": "^3.0.3",
    "@octokit/plugin-throttling": "^3.3.1",
    "@octokit/rest": "^18.0.6",
    "webextension-polyfill": "^0.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "autoprefixer": "^10.0.0",
    "auto-changelog": "^2.2.0",
    "cpx2": "^3.0.0",
    "gh-release": "^4.0.0",
    "mkdirp": "^1.0.4",
    "npm-run-all2": "^5.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.29.0",
    "standard": "^16.0.2",
    "tap": "^14.10.8",
    "postcss": "^8.0.3",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-cli": "^8.0.0",
    "postcss-import": "^13.0.0",
    "postcss-nesting": "^7.0.0",
    "postcss-reporter": "^7.0.0",
    "postcss-url": "^10.0.0",
    "mine.css": "^4.5.0"
  },
  "homepage": "https://github.com/bcomnes/GitMutual",
  "keywords": [],
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcomnes/GitMutual.git"
  },
  "scripts": {
    "release": "git push --follow-tags && gh-release -y",
    "test": "run-s test:*",
    "test:standard": "standard",
    "test:tape": "tap",
    "version": "run-s prepare && auto-changelog -p --template keepachangelog auto-changelog --breaking-pattern 'BREAKING CHANGE:' && git add CHANGELOG.md dist",
    "prepare": "npm run build",
    "clean": "rimraf dist/* && mkdirp dist",
    "build": "run-s clean build:*",
    "build:js": "rollup --config rollup.config.js",
    "build:static": "cpx 'src/**/*.{html,json,png}' dist",
    "build:css-options": "postcss src/options.css -o dist/options.css",
    "build:css-popup": "postcss src/popup.css -o dist/popup.css",
    "build:css-unfollowers": "postcss src/unfollowers.css -o dist/unfollowers.css",
    "watch": "run-s clean && run-p watch:*",
    "watch:js": "npm run build:js -- -w --no-watch.clearScreen",
    "watch:static": "npm run build:static -- -w",
    "watch:css-options": "npm run build:css-options -- -w",
    "watch:css-popup": "npm run build:css-popup -- -w",
    "watch:css-unfollowers": "npm run build:css-unfollowers -- -w",
    "start": "npm run watch"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "browser",
      "chrome"
    ]
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/bcomnes"
  }
}
